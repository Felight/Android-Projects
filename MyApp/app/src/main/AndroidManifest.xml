<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.felight.myapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/felight_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GreetUserActivity">
        </activity>
        <activity
            android:name=".NewActivity"
            android:label="New Activity">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Main Activity">
        </activity>
        <activity
            android:name=".SimpleCalculatorActivity"
            android:label="@string/calc_title">
        </activity>
        <activity
            android:name=".AlgoBenchmarkingActivity"
            android:label="Algorithm Benchmarking App">

        </activity>
        <activity android:name=".ActivityLifeCycleDemo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>